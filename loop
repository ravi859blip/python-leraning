name = "abhishek"
for i in name:
    print(i)
    if i == "k":
        print("this is a special case")
    
colors  = ["red", "green", "blue",]
for colors in colors:
    print(colors)
    for i in colors:
        print(i)
        if i == "k":
            print("this is a special case")

for k in range(5):
    print(k+1)
for k in range(5,100):
    print(k)

for i in range(1,12,3):
    print(i)
